{
  "devDependencies": {
    "@astrojs/cloudflare": "^12.6.10",
    "@astrojs/mdx": "^4.3.7",
    "@astrojs/tailwind": "^6.0.2",
    "astro": "^5.14.5",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "wrangler": "^4.43.0",
    "zod": "^3.25.76"
  },
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "cf:dev": "wrangler pages dev .",
    "cf:deploy": "wrangler pages deploy .",
    "deploy": "wrangler pages deploy dist --project-name roost-property-management-website",
    "doctor": "bash scripts/doctor.sh",
    "deploy:prod": "wrangler pages deploy dist --project-name roost-property-management-website --branch main --commit-dirty=true",
    "verify": "bash bin/verify.sh",
    "ship": "npm run build && npm run deploy:prod && npm run verify"
  }
}