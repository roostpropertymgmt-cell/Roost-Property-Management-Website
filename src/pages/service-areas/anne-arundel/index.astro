---
import Layout from "../../../components/Layout.astro";
const title = "Anne Arundel County Service Areas";
const description = "Roost Property Management services in Anne Arundel County: Annapolis, Severna Park, Glen Burnie.";
const items = [
  { name: "Annapolis", url: "/service-areas/anne-arundel/annapolis" },
  { name: "Severna Park", url: "/service-areas/anne-arundel/severna-park" },
  { name: "Glen Burnie", url: "/service-areas/anne-arundel/glen-burnie" },
];
const json = {
  "@context": "https://schema.org",
  "@type": "ItemList",
  "name": "Anne Arundel County Service Areas",
  "itemListElement": items.map((it, i) => ({
    "@type": "ListItem",
    "position": i + 1,
    "item": { "@type": "WebPage", "name": it.name, "url": `https://roost-property-management-website.pages.dev${it.url}` }
  }))
};
---
<Layout {title} {description}>
  <h1>{title}</h1>
  <p>{description}</p>
  <ul>
    {items.map((it) => <li><a href={it.url}>{it.name}</a></li>)}
  </ul>
  <script type="application/ld+json">{JSON.stringify(json)}</script>
</Layout>

<!-- Added by script: extra links to new service areas -->
<section class="container">
  <h2>More Anne Arundel Service Areas</h2>
  <ul>
    <li><a href="/service-areas/anne-arundel/severna-park/">Severna Park Property Management</a></li>
    <li><a href="/service-areas/anne-arundel/glen-burnie/">Glen Burnie Property Management</a></li>
  </ul>
</section>

<script type="application/ld+json" set:html={JSON.stringify({
  "@context":"https://schema.org",
  "@type":"ItemList",
  "itemListElement":[
    {"@type":"ListItem","position":1,"url":"/service-areas/anne-arundel/severna-park/","name":"Severna Park"},
    {"@type":"ListItem","position":2,"url":"/service-areas/anne-arundel/glen-burnie/","name":"Glen Burnie"}
  ]
})}></script>
